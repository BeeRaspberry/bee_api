mutation {
  createStateProvince(input:{
    name: "Aberdeen City",
    country: "Scotland"
  }) {
    StateProvince {
      id
    }
  }
}

{
  countryList {
    pageInfo {
      hasNextPage
      hasPreviousPage
    }
    edges {
      node {
        id
        name
        stateProvinces {
          edges {
            node {
              id
              name
            }
          }
        }
      }
    }
  }
}